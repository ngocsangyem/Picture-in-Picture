{
	"name": "parcel-boilerplate",
	"version": "1.0.0",
	"description": "Front-end starter",
	"main": "index.js",
	"scripts": {
		"start": "parcel src/index.html --open",
		"clean": "rm -rf dist/*",
		"build:parcel": "parcel build src/index.html --no-content-hash --no-minify --no-source-maps --public-url ./",
		"build": "npm run clean && npm run build:parcel"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ngocsangyem/parcel-boilerplate.git"
	},
	"keywords": [
		"javascript",
		"html",
		"css",
		"sass",
		"parcel",
		"scss",
		"ES6"
	],
	"author": "ngocsanngyem",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ngocsangyem/parcel-boilerplate/issues"
	},
	"homepage": "https://github.com/ngocsangyem/parcel-boilerplate#readme",
	"devDependencies": {
		"parcel": "^1.12.4",
		"sass": "^1.26.12",
		"typescript": "^4.0.3"
	}
}
